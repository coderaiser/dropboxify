{
  "name": "dropboxify",
  "version": "2.0.2",
  "main": "lib/dropboxify.js",
  "description": "Read directory content from dropbox with file attributes",
  "author": "",
  "homepage": "http://github.com/coderaiser/dropboxify",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/dropboxify.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "lint": "redrun lint:*",
    "lint:eslint:lib": "eslint lib",
    "lint:eslint:test": "eslint -c .eslintrc.test test --no-eslintrc"
  },
  "dependencies": {
    "@cloudcmd/formatify": "^1.0.0",
    "@cloudcmd/sortify": "^2.0.0",
    "currify": "^4.0.0",
    "date-fns": "^1.15.0",
    "dropbox": "^4.0.0",
    "format-io": "^2.0.0",
    "node-fetch": "^2.2.0",
    "shortdate": "^1.2.0"
  },
  "devDependencies": {
    "@cloudcmd/stub": "^2.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^6.4.0",
    "mock-require": "^3.0.2",
    "nodemon": "^1.14.11",
    "nyc": "^13.1.0",
    "try-to-catch": "^1.0.2"
  },
  "keywords": [
    "dropbox",
    "directory",
    "read",
    "readdir",
    "readify",
    "cloudcmd"
  ],
  "engines": {
    "node": ">=8"
  },
  "license": "MIT"
}
