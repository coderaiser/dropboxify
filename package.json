{
  "name": "dropboxify",
  "version": "1.0.5",
  "main": "lib/dropboxify.js",
  "description": "Read directory content from dropbox with file attributes",
  "author": "",
  "homepage": "http://github.com/coderaiser/dropboxify",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/dropboxify.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "lint": "redrun lint:*",
    "lint:eslint:lib": "eslint lib",
    "lint:eslint:test": "eslint -c .eslintrc.test test --no-eslintrc"
  },
  "dependencies": {
    "@cloudcmd/formatify": "^1.0.0",
    "@cloudcmd/sortify": "^2.0.0",
    "currify": "^3.0.0",
    "date-fns": "^1.15.0",
    "dropbox": "^4.0.0",
    "format-io": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "shortdate": "^1.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-node": "^8.0.0",
    "mock-require": "^3.0.2",
    "nodemon": "^1.14.11",
    "nyc": "^13.1.0",
    "redrun": "^7.0.2",
    "sinon": "^7.1.0",
    "sinon-called-with-diff": "^2.0.0",
    "tape": "^4.6.3",
    "try-to-catch": "^1.0.2"
  },
  "keywords": [
    "dropbox",
    "directory",
    "read",
    "readdir",
    "readify",
    "cloudcmd"
  ],
  "engines": {
    "node": ">=8"
  },
  "license": "MIT"
}
